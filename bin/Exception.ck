/**
*  Exception class
*  @author Iain Emsley 
*/

public class Exception 
{   
    string except;

    /**
    * Method to create an exception
    */
    fun string createException(string message)
    {
        message => except;
    }

    /**
    * Function to print the exception
    */
    fun void print()
    {
        <<< except >>>;
    }
}
